					"code" : "inlets = 1;\noutlets = 2;\r\n\r\nvar _threshold = 0.5;\n\nfunction autocorrelate(deltas) {\n    let n = deltas.length;\n    if (n < 2) {\n        outlet(0, []);\n        return;\n    }\n\n    // Compute mean and normalize\n    let mean = deltas.reduce((sum, x) => sum + x, 0) / n;\n    let normalized = deltas.map(x => x - mean);\n\n    // Autocorrelation computation\n    let result = new Array(n).fill(0);\n    for (let lag = 0; lag < n; lag++) {\n        let sum = 0;\n        for (let i = 0; i < n - lag; i++) {\n            sum += normalized[i] * normalized[i + lag]; // Dot product\n        }\n        result[lag] = sum / (n - lag);\n    }\n\n    return normalizeArray(result);\n}\n\n// Normalize to range -1 to 1\nfunction normalizeArray(arr) {\n    let maxVal = Math.max(...arr.map(Math.abs));\n    return arr.map(x => x / maxVal);\n}\n\n// Detect peaks in autocorrelation\nfunction findPeaks(arr) {\n    let peaks = [];\n    for (let i = 1; i < arr.length - 1; i++) {\n        if (arr[i] > arr[i - 1] && arr[i] > arr[i + 1] && arr[i] > _threshold) {\n            peaks.push(i); // The lag where repetition is detected\n        }\n    }\n    return peaks;\n}\n\r\nfunction threshold(v) {\r\n\t_threshold = v;\r\n}\r\n\n// Main function to handle input\nfunction list() {\n    let deltas = arrayfromargs(arguments);\n    let acf = autocorrelate(deltas); // Full-length array\n    let peaks = findPeaks(acf); // Only peak indices\n\n    // Output both: Full ACF + Peaks\n    outlet(0, \"acf\", acf); // Full-length autocorrelation output\n    outlet(1, \"peaks\", peaks); // Just detected repeating lags\n}",
